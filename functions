def trim(str):
    return str.strip()  # Removes leading and trailing spaces

def exists(str, x):
    return str.find(x)  # Returns index of first occurrence of x in str

def titleIt(str):
    return str.title()  # Converts first letter of each word to uppercase

def casesSwap(str):
    return str.swapcase()  # Swaps case of each character
